        screen-active = &apos;1&apos;.
        screen-invisible = &apos;0&apos;.
        MODIFY SCREEN.
      ELSEIF screen-group1 = &apos;NNM&apos; OR screen-group1 =&apos;NPG&apos;
         OR screen-group1 = &apos;RNM&apos;.
        screen-active = &apos;0&apos;.
